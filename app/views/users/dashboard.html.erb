<div class="dashbord_container">
    <ul class="left_bar">
      <% if current_user.store.nil?%>
        <li><%= link_to "crée la boutique", new_store_path, class: "dasboard_button", remote: true %></li>
      <% else %>
        <li><%= link_to "mise a jours de la boutique", root_path, class: "dasboard_button", remote: true %></li>
      <% end %>
      <% if current_user.store.nil? ==  false %>
        <li><%= link_to "crée un produit", new_store_product_path(current_user.store.id), class: "dasboard_button", remote: true %></li>
        <li><%= link_to "mise a jours des produits", root_path, class: "dasboard_button", remote: true %></li>
        <li><%= link_to "commande", root_path , class: "dasboard_button", remote: true %></li>
        <li><%= link_to "historique des commande", root_path , class: "dasboard_button", remote: true %></li>
      <% end %>
        <li><%= link_to "abonnement", root_path, class: "dasboard_button" %></li>
    </ul>

  <div class="order_container" id="dashboard_partial">
    <h1>Bienvenu dans votre tableau de bord !</h1>
  </div>
</div>
